# Docker Compose environment variables for EasyBody backend

# Spring profile selection
SPRING_PROFILES_ACTIVE=local

# Database connection for both services
DB_HOST=db
DB_PORT=5432
DB_NAME=easybody
DB_USERNAME=postgres
DB_PASSWORD=postgres

# Published port for the Spring Boot service
APP_PORT=8080

# Toggle AWS features (disabled for local development)
AWS_ENABLED=false
